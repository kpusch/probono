body,
div {
  border: 0 none;
  padding: 0;
  margin: 0;
  color: floralwhite;
}

html,
body {
  height: 100%;
  min-height: 100%;
  overflow: hidden;
}

#wrap {
    display: -webkit-flex;
	display: flex;
	flex-direction: row;
	flex-wrap: wrap;
    margin: 0 auto;
    overflow: hidden;
    width: 100%;
    height: 100%;
    min-height: 100%;
    transition: margin-left .5s;
    background-color:#009ED8;
}

#header {
    display: -webkit-flex;
	display: flex;
	flex-direction: row;
	flex-wrap: wrap;
    width: 100%;
    height: 5%;
    min-height: 40px;
    background-color:#009ED8;
}

#header .tabs {
    display: -webkit-flex;
	display: flex;
	flex-direction: row;
	flex-wrap: wrap;
    width:90%;
}

#header .btnpadding {
    width:10%;
}

#header .tabs ul {
    list-style-type: none;
    padding: 0;
    margin: 0;
}

#header .tabs li {
    display: inline-block;
}

#header button {
    height:40px;
    font-size:1vw;
}

#header .sidemenu {
    display: -webkit-flex;
	display: flex;
    -webkit-flex-direction: row-reverse;
	flex-direction: row-reverse;
    width:10%;
    text-align: right;
}

#opensidemenu {
    display:block;
    width:50%;
    height:100%;
}

#opensidemenu button {
    background-color:#009ED8;
}

#out {
    display: block;
    width:50%;
    height:100%;
}

#out button {
    background-color:#009ED8;
}

#navout {
    display: none;
    width:50%;
    height:100%;
}

#navout button {
    background-color:#009ED8;
}

#header button {
  display: inline-block;
  font-family: "Montserrat", "Trebuchet MS", Helvetica, sans-serif;
  -webkit-font-smoothing: antialiased;
  position: relative;
  border: none;
  
  color: white;
  transition: .2s;
}

#header button:before,
#header button:after {
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  padding-top: inherit;
  padding-bottom: inherit;
  width: 100%;
  content: "\00a0";
  font-family: 'FontAwesome', sans-serif;
  font-size: 1.2em;
  text-align: center;
  transition: .2s;
  transform-origin: 50% 60%;
}
#header button:before {
  background: rgba(0, 0, 0, 0.1);
}
#header button:hover {
  background: #0079a5;
}
#header button:active,
#header button:focus {
  background: #002e3f;
  outline: none;
}

#tabbtn1 {
    background: #002e3f;
}

#tabbtn2 {
    background: #009ED8;
}

#tabbtn3 {
   background: #009ED8;
}

#tabbtn4 {
    background: #009ED8;
}

#contentbox {
    display: -webkit-flex;
	display: flex;
	flex-direction: row;
	flex-wrap: wrap;
    width:100%;
    height:95%;
}

#userviewbox {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
    width:15%;
    height:100%;
    background-color:blue;
    border-right: 1px solid black;
}

#userviewbox .userscreen {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
    width:100%;
    height:19.9%;
	border-bottom: 0.1px solid white;
    background-color:black;
}

#userviewbox .userview {
