                    <li>
                        <a class="page-scroll" href="#contact">Contact</a>
                    </li>
                </ul>
            </div>
            <!-- Contact div -->
            <div id="contactform">
                <div class="contactinputarea">
                    <div class="contactTitle">
                        Contact
                    </div>

                    <div class="contactInput">
                        <input type="text" placeholder="E-Mail" style="width:250px; height:35px; font-size:18px; color:black;">
                    </div>
                    <div class="contactInput">
                        <input type="text" placeholder="Name" style="width:250px; height:35px; font-size:18px; color:black;">
                    </div>
                    <div class="contactInput">
                        <input type="text" placeholder="Phone number" style="width:250px; height:35px; font-size:18px; color:black;">
                    </div>
                    <div class="contactInput">
                       <textarea rows="5" placeholder="Message" id="message" style="width:250px; height:100px; font-size:18px; color:black;"></textarea>
                    </div>
                    <div class="forbtnpadding">
                       <button type="submit" class="contactBtn">Send</button>
                    </div>
                    <div class="forbtnpadding">
                       <button id="closeBtn" class="contactBtn">Close</button>
                    </div>
